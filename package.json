{
  "name": "sls-identity-api",
  "version": "1.0.0",
  "description": "Serverless Identity & Access Management API",
  "scripts": {
    "start": "serverless offline",
    "deploy:dev": "serverless deploy --aws-profile personal --stage dev",
    "deploy:production": "serverless deploy --aws-profile personal --stage production",
    "test": "exit 0",
    "lint": "eslint '*/**/*.{js,ts}'",
    "prettier": "prettier --write '*/**/*.{js,ts}'",
    "docs": "redoc-cli serve docs/openapi/openapi.yml",
    "docs:watch": "redoc-cli serve docs/openapi/openapi.yml --watch"
  },
  "engines": {
    "node": "16.x",
    "yarn": ">= 1.19.1"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "1.4.0",
    "@aws-sdk/client-cognito-identity-provider": "3.198.0",
    "@aws-sdk/client-dynamodb": "3.198.0",
    "@aws-sdk/util-dynamodb": "3.198.0",
    "@middy/core": "3.6.2",
    "@middy/http-header-normalizer": "3.6.2",
    "@middy/http-json-body-parser": "3.6.2",
    "@middy/http-security-headers": "3.6.2",
    "@schibsted/middy-error-handler": "4.2.2",
    "aws-jwt-verify": "3.2.0",
    "aws-lambda": "1.0.7",
    "http-errors": "2.0.0",
    "jsonschema": "1.4.1",
    "lodash": "4.17.21",
    "serverless": "3.23.0",
    "source-map-support": "0.5.19"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@serverless/typescript": "3.21.0",
    "@types/aws-lambda": "8.10.108",
    "@types/lodash": "4.14.186",
    "@types/node": "14.14.6",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "commitlint": "11.0.0",
    "esbuild": "0.15.12",
    "eslint": "7.17.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "fork-ts-checker-webpack-plugin": "6.0.0",
    "husky": "4.3.7",
    "json-schema-to-ts": "1.4.1",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "redoc-cli": "0.13.2",
    "serverless-add-api-key": "^4.2.1",
    "serverless-esbuild": "1.33.0",
    "serverless-offline": "11.2.1",
    "serverless-plugin-canary-deployments": "0.7.1",
    "serverless-prune-plugin": "1.5.0",
    "serverless-s3-sync": "^3.1.0",
    "serverless-stage-manager": "1.0.5",
    "serverless-webpack": "5.2.0",
    "ts-loader": "8.0.10",
    "ts-node": "9.1.1",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "typescript": "4.0.5",
    "webpack": "5.4.0",
    "webpack-node-externals": "2.5.2"
